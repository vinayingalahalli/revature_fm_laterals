package com.demo.service;

import org.apache.log4j.Logger;

import com.demo.Log4jAppMain;

public class ServiceClass {
	private static Logger log=Logger.getLogger(ServiceClass.class);

	public boolean isPrime(int n) {
		int c=0;
		boolean b=false;
		for (int i = 1; i <=n; i++) {
			if(n%i==0) {
				c++;
			}
		}
		if(c==2) {
			b=true;
		}
		return b;
	}
}
